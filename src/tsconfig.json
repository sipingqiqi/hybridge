{
  "compilerOptions": {
    "module": "es2015",
    "target": "es2015",
    "lib": [
      "es2016",
      "dom"
    ],
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "outDir": "../es6"
  },
  "exclude": [
    "node_modules"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit": true
  }
}